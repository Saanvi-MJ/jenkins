pipeline {
    agent any
    
    stages {
        stage('Build') {
            steps {
                echo 'Building automatically again '
                echo 'Welcome Nisha '
            }
        }
 stage('Test') {
            steps {
                echo 'Testing automatically again'
            }
        }
 stage('Deploy') {
            steps {
                echo 'Deploying automatically'
            }
        }
stage('Development') {
    steps {
        echo 'Developed automatically'
        echo 'Successful'
    }
}
}
}
